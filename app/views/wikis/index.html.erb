
<div class="container">
<div class="starter-template">


 <h1>Wikis</h1>

 <div class="row">
   <div class="col-md-8">
 <!-- #7 -->
     <% @wikis.each do |wiki| %>
       <div class="media">
         <div class="media-body">
           <h4 class="media-heading">
 <!-- #8 -->
             <%= link_to wiki.title, wiki %>
           </h4>
          
         </div>
       </div>
     <% end %>
   </div>
  
   <div class="col-md-4">
     <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
   </div>
 </div>


</div>
</div>

